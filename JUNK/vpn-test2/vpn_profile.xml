<?xml version="1.0" encoding="UTF-8"?>
<openconnect>
    <server>$VPN_HOST</server>
    <protocol>anyconnect</protocol>
    <port>443</port>
    <user>$VPN_USER</user>
    <password>$VPN_PASS</password>
    <authgroup>group_name</authgroup>
    <tokenmode>none</tokenmode>
    <passwd-on-stdin>true</passwd-on-stdin>
    <no-cert-check>true</no-cert-check>
    <useragent>OpenConnect XML Example</useragent>
    <timestamp>true</timestamp>
</openconnect>